import k3d

import sys
import os
sys.path.append(os.path.join(str(k3d.share_path()), &quot;scripts&quot;, &quot;animation&quot;))

from keyframes import *

# Duration of the move
d = 4

curve = nucurve(4)
curve.control_points.append(control_point(k3d.vector3(-13, 0, 0), 1.0)) # First keyframe
curve.control_points.append(control_point(k3d.vector3(-13, -10, 0), 1.0)) # Tangent
curve.control_points.append(control_point(k3d.vector3(13, 10, 0), 1.0))   # Tangent
curve.control_points.append(control_point(k3d.vector3(13, 0, 0), 1.0))   # Second keyframe
curve.knots = [0, 0, 0, 0, d, d, d, d] # Bezier knots

Output = k3d.translate3(evaluate(curve, Node.time))</property>
